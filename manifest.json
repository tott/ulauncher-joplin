{
  "required_api_version": "^2.0.0",
  "name": "Joplin notes",
  "description": "Easily add notes to joplin using ulauncher. Originally created by Denis Gerber.",
  "developer_name": "Thorsten Ott",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "kw_note",
      "type": "keyword",
      "name": "Joplin Note",
      "default_value": "note"
    },
    {
      "id": "kw_todo",
      "type": "keyword",
      "name": "Joplin ToDo",
      "default_value": "todo"
    },
    {
      "id": "server",
      "type": "input",
      "name": "Joplin Server Address",
      "default_value": "http://localhost:41184"
    },
    {
      "id": "token",
      "type": "input",
      "name": "Joplin API Token",
      "default_value": ""
    },
    {
      "id": "default_note_notebook",
      "type": "input",
      "name": "Default Notebook ID for Notes (optional)",
      "default_value": ""
    },
    {
      "id": "default_todo_notebook",
      "type": "input",
      "name": "Default Notebook ID for Todos (optional)",
      "default_value": ""
    },
    {
      "id": "limit",
      "type": "input",
      "name": "Count of notebooks that should be shown",
      "default_value": "5"
    }
  ]
}
  
